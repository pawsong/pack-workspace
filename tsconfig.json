{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "lib"
  }
}

